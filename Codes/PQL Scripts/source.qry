#DEFINE file_path 'E:\\Personal_Drive_Backup\\My Important Files\\Study\\Uni-Bamberg\\Thesis\\Odysseus\\Benchmarking\\Dataset\\Processed\\sensor_6687_processed.csv'

#RUNQUERY
sensor_source ::= ACCESS({
    source = 'sensor_source',
    wrapper = 'GenericPull',
    transport = 'File',
    protocol = 'CSV',
    datahandler = 'Tuple',
    options = [
		['filename', ${file_path}],
        ['separator', ','],
///        ['nanodelay', '10'],
///        ['scheduler.delay', '1'],
        ['skipFirstLines','1']
    ],
    schema = [
        ['value_id', 'Integer'],
        ['sensor_id', 'Integer'],
        ['timestamp', 'Timestamp'],
        ['value', 'Double'],
        ['available_time', 'Timestamp']
    ]
})